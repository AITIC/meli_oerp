<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report_shipment">
					<t t-call="web.html_container">
							<t t-foreach="docs" t-as="o">
									<t t-call="web.external_layout">
											<div class="page">
													<h2>Shipment</h2>
													<p>Destinatario <span t-field="o.receiver_address_name"/></p>
													<p>Dirección <span t-field="o.receiver_street_name"/></p>
													<p>Teléfono <span t-field="o.receiver_address_phone"/>; Destino <span t-field="o.receiver_city"/>,<span t-field="o.receiver_state"/>,<span t-field="o.receiver_country"/></p>
													<p>Tipo de envío <span t-field="o.tracking_method"/></p>
													<p>Productos ,,,</p>
													<p>Remitente SLTSLC xxxx, yyy ,zzz</p>
											</div>
									</t>
							</t>
					</t>
        </template>
    </data>
</odoo>
