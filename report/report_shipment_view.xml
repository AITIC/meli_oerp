<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report_shipment">
					<t t-call="web.html_container">
						<div class="page">
							<t t-foreach="docs" t-as="o">
										<table>
											<tr>
												<td>
													<table>
														<tr>
															<td>
																<p>Destinatario <span t-field="o.receiver_address_name"/></p>
															</td>
														</tr>
														<tr>
															<td>
																<p>Dirección <span t-field="o.receiver_street_name"/></p>
															</td>
														</tr>
														<tr>
															<td>
																<table>
																	<tr>
																		<td>Teléfono <span t-field="o.receiver_address_phone"/></td>
																		<td>Destino <span t-field="o.receiver_city"/>,<span t-field="o.receiver_state"/>,<span t-field="o.receiver_country"/></td>
																	</tr>
																</table>
															</td>
														</tr>
														<tr>
															<td>
																<p>Tipo de envío <span t-field="o.tracking_method"/></p>
															</td>
														</tr>
														<tr>
															<td>
																<p>Productos ,,,</p>
															</td>
														</tr>
													</table>
												</td>
											</tr>
										<table>
									</t>
						</div>
					</t>
        </template>
    </data>
</odoo>
